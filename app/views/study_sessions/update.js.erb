<% if @study_session.to_dos.any? %>
  $('#card-partial').html('<%= j render partial: 'cards/card' %>')
  $('#response').html('<%= j render partial: 'user_cards/response' %>')
<% else %>
  $('#pepper-main').html('<%= j render partial: 'study_sessions/session_complete' %>');  
  $(document).ready(function(){
    $('#navigation-id').show();
  });
<% end %>