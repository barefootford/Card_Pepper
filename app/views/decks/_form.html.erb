<%= form_for @deck do |f| %>
<%= render "layouts/errors", object: @deck %>

  <p>
    <%= f.label :title %><br>
    <%= f.text_area :title, cols: 40 %>
    <br>
  </p>
  <p>
    <%= f.label :instructions %><br>
    <%= f.text_area :instructions, cols: 40, rows: 4 %>
    <br>
  </p>
  <%= f.submit %>
<% end %>